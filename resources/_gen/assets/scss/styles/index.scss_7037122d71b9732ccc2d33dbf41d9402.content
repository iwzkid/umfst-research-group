*,*::before,*::after{box-sizing:border-box}*{margin:0;padding:0}html,body{height:100%}body{line-height:1.5;-webkit-font-smoothing:antialiased}img,picture,video,canvas,svg{display:block;max-width:100%}input,button,textarea,select{font:inherit}p,h1,h2,h3,h4,h5,h6{overflow-wrap:break-word}@font-face{font-family:wotfardregular;src:url(fonts/wotfard-regular/wotfard-regular-webfont.eot);src:url(fonts/wotfard-regular/wotfard-regular-webfont.eot?#iefix)format("embedded-opentype"),url(fonts/wotfard-regular/wotfard-regular-webfont.woff2)format("woff2"),url(fonts/wotfard-regular/wotfard-regular-webfont.ttf)format("truetype");font-weight:400;font-style:normal}:root{--background-primary:#ffffff;--background-secondary:#13181F;--text-normal-primary:#000000;--text-normal-secondary:#7E8C9A;--text-accent:#4C6EF5;--text-hover:#F5D34C;--title-text-color:#FF0878;--description-text-color:#eeeeee}:root{--general-transition-time:0.15s;--hover-transition-time:0.15s}:root{--site-wrapper-width:768px;--site-wrapper-width-max:768px;--navbar-height:80px}:root{--default-font:"wotfardregular";--font-size-xxsm:14px;--font-size-xsm:16px;--font-size-sm:18px;--font-size-m:20px;--font-size-lg:24px;--font-size-xlg:28px;--font-size-xxlg:32px}body{display:grid;background-color:var(--background-primary);font-family:var(--default-font);color:var(--text-normal-primary)}.site-wrapper{display:grid;grid-template-rows:auto 1fr auto;justify-self:center;gap:20px;padding:0 60px;padding-top:60px;width:85%;height:100%;transition:width var(--general-transition-time),transform var(--general-transition-time)}@media screen and (max-width:1220px){.site-wrapper{width:100%;padding:0 30px;padding-top:30px}}header .navbar{display:grid;grid-template-columns:auto 1fr}header .navbar .logo a{text-transform:uppercase;justify-self:end;text-decoration:none;color:var(--text-normal-primary);font-size:var(--font-size-lg);font-weight:700}header .navbar .logo a.desktop{display:block}@media screen and (max-width:768px){header .navbar .logo a.desktop{display:none}}header .navbar .logo a.mobile{display:none}@media screen and (max-width:768px){header .navbar .logo a.mobile{display:block}}header .navbar .links{justify-self:end}header .navbar .links ul li{list-style:none;display:grid;margin-bottom:5px}header .navbar .links ul li a{text-transform:uppercase;justify-self:end;text-decoration:none;color:var(--text-normal-primary);font-size:var(--font-size-sm)}header .navbar .links ul li a:hover{text-decoration:underline}footer.page-footer{padding-bottom:30px}